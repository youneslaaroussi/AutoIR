{
  "name": "autoir",
  "description": "Incident Reporting for AWS using AI Agents powered by TiDB and Kimi K2.",
  "version": "1.0.0",
  "author": "Younes Laaroussi",
  "bin": {
    "autoir": "./bin/run.js"
  },
  "bugs": "https://github.com/youneslaaroussi/autoir/issues",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.864.0",
    "@aws-sdk/client-sns": "^3.864.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.864.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.5.0",
    "clipboardy": "^4.0.0",
    "enquirer": "^2.4.1",
    "inquirer": "^12.9.2",
    "mysql2": "^3.11.3",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/blessed": "^0.1.25",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "jest": "^30.0.5",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin/run.js",
    "./bin/run.cmd",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/youneslaaroussi/autoir",
  "keywords": [
    "oclif",
    "aws",
    "incident-reporting",
    "ai-agents",
    "tidb",
    "kimi",
    "cli",
    "monitoring",
    "observability"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "autoir",
    "dirname": "autoir",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "youneslaaroussi/autoir",
  "scripts": {
    "build": "tsc -b --clean && tsc -b",
    "lint": "eslint",
    "postpack": "node -e \"try{require('fs').unlinkSync('oclif.manifest.json')}catch(e){}\"",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
